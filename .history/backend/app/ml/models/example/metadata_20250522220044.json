    "6": "Emphysema",
    "7": "Fibrosis",
    "8": "Hernia",
    "9": "Infiltration",
    "10": "Mass",
    "11": "Nodule",
    "12": "Pleural_Thickening",
    "13": "Pneumonia"
  },
  "preprocessing": {
    "normalize": true,
    "target_size": [224, 224]
  }
}
