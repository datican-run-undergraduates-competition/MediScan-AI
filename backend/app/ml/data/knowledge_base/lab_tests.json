                    },
                    "bun": {
                        "normal_range": "7-20 mg/dL",
                        "units": "mg/dL"
                    },
                    "egfr": {
                        "normal_range": ">60 mL/min/1.73m²",
                        "units": "mL/min/1.73m²"
