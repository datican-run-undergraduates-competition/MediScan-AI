{
    "complete_blood_count": {
        "name": "Complete Blood Count (CBC)",
        "description": "Comprehensive blood test that measures various components of blood",
        "category": "Hematology",
        "components": {
            "red_blood_cells": {
                "name": "Red Blood Cells (RBC)",
                "normal_range": "4.5-5.5 million/µL",
                "units": "million/µL",
                "purpose": "Measure oxygen-carrying capacity",
                "abnormal_conditions": {
                    "high": [
                        "Polycythemia",
                        "Dehydration",
                        "Lung disease"
                    ],
                    "low": [
                        "Anemia",
                        "Blood loss",
                        "Bone marrow disorders"
                    ]
                }
            },
            "white_blood_cells": {
                "name": "White Blood Cells (WBC)",
                "normal_range": "4,500-11,000/µL",
                "units": "/µL",
                "purpose": "Measure immune system function",
                "abnormal_conditions": {
                    "high": [
                        "Infection",
                        "Inflammation",
                        "Leukemia"
                    ],
                    "low": [
                        "Bone marrow suppression",
                        "Autoimmune disorders",
                        "Viral infections"
                    ]
                }
            },
            "platelets": {
                "name": "Platelets",
                "normal_range": "150,000-450,000/µL",
                "units": "/µL",
                "purpose": "Measure blood clotting ability",
                "abnormal_conditions": {
                    "high": [
                        "Inflammation",
                        "Iron deficiency",
                        "Cancer"
                    ],
                    "low": [
                        "Bleeding disorders",
                        "Bone marrow disorders",
                        "Autoimmune conditions"
                    ]
                }
            },
            "hemoglobin": {
                "name": "Hemoglobin",
                "normal_range": "13.5-17.5 g/dL",
                "units": "g/dL",
                "purpose": "Measure oxygen-carrying protein",
                "abnormal_conditions": {
                    "high": [
                        "Dehydration",
                        "Polycythemia",
                        "Lung disease"
                    ],
                    "low": [
                        "Anemia",
                        "Blood loss",
                        "Nutritional deficiencies"
                    ]
                }
            },
            "hematocrit": {
                "name": "Hematocrit",
                "normal_range": "38.8-50%",
                "units": "%",
                "purpose": "Measure percentage of blood volume occupied by red blood cells",
                "abnormal_conditions": {
                    "high": [
                        "Dehydration",
                        "Polycythemia",
                        "Lung disease"
                    ],
                    "low": [
                        "Anemia",
                        "Blood loss",
                        "Nutritional deficiencies"
                    ]
                }
            }
        },
        "preparation": [
            "Fasting not required",
            "Avoid strenuous exercise",
            "Stay hydrated"
        ],
        "interpretation": {
            "normal": "All components within reference ranges",
            "abnormal": "Any component outside reference ranges",
            "critical": "Severe abnormalities requiring immediate attention"
        }
    },
    "comprehensive_metabolic_panel": {
