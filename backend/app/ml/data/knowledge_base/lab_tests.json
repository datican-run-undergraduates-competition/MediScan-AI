                        "units": "U/L"
                    },
                    "ast": {
                        "normal_range": "10-40 U/L",
                        "units": "U/L"
                    },
                    "alkaline_phosphatase": {
                        "normal_range": "44-147 U/L",
                        "units": "U/L"
                    },
                    "bilirubin": {
                        "normal_range": "0.1-1.2 mg/dL",
                        "units": "mg/dL"
                    }
