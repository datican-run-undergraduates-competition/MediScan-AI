            "electrolytes": {
                "name": "Electrolytes",
                "components": {
                    "sodium": {
                        "normal_range": "135-145 mEq/L",
                        "units": "mEq/L"
                    },
                    "potassium": {
                        "normal_range": "3.5-5.0 mEq/L",
                        "units": "mEq/L"
                    },
                    "chloride": {
                        "normal_range": "98-106 mEq/L",
                        "units": "mEq/L"
                    },
                    "co2": {
                        "normal_range": "23-29 mEq/L",
                        "units": "mEq/L"
                    }
                },
                "abnormal_conditions": {
                    "high": [
                        "Dehydration",
                        "Kidney disease",
                        "Adrenal disorders"
                    ],
                    "low": [
                        "Diarrhea",
                        "Vomiting",
                        "Kidney disease"
                    ]
                }
            }
        },
        "preparation": [
            "Fasting for 8-12 hours",
            "Avoid alcohol",
            "Stay hydrated"
        ],
        "interpretation": {
            "normal": "All components within reference ranges",
            "abnormal": "Any component outside reference ranges",
            "critical": "Severe abnormalities requiring immediate attention"
        }
    },
