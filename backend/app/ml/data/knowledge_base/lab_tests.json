                    "low": [
                        "Hypoglycemia",
                        "Liver disease",
                        "Insulin overdose"
                    ]
                }
            },
            "kidney_function": {
                "name": "Kidney Function Tests",
                "components": {
                    "creatinine": {
                        "normal_range": "0.7-1.3 mg/dL",
